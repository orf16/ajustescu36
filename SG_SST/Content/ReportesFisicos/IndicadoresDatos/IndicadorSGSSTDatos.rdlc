  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
[PK_Id_ActaRevision],
[Fecha],
[FechaCierre],
[Fk_Id_Empresa],
[Nit_Empresa],
--Pk_Id_ModuloPlanAccion,
[FK_Acta],
[Responsable],
 YEAR(Fecha) AS Anio, 
MONTH(FechaCierre) AS Mes, 
YEAR(FechaCierre) AS AnioCierre, 
MONTH(FechaCierre) AS MesCierre,
Dense_Rank()over(order by FK_Acta asc) as ConsecutivoPlan,
[Elaborada],
[Observaciones],
 CASE 
WHEN (V_IndicadorSGSST_OrigenDatos.FechaCierre) IS NULL THEN 'ABIERTAS' ELSE 'CERRADAS' END AS Estado_Plan, 
SUM(CASE WHEN FechaCierre  is null THEN 1 ELSE 0 END) AS SinCerrar,
SUM(CASE WHEN FechaCierre  is not null THEN 1 ELSE 0 END) AS SeCerro,

case when (FechaCierre is null) and (Fecha&gt;=GETDATE()) then 'Abierto-Vigente' 
 else 
 case when (FechaCierre is null) and (Fecha&lt;=GETDATE()) then 'Abierto-Vencido' 
   else
   case when (FechaCierre is not null) and (Fecha&gt;=FechaCierre) then 'Eficaz'   	

   else 'No Eficaz' 
end
end 
end as EstadoPlan,
count(*) as total
from V_IndicadorSGSST_OrigenDatos	

group by
[PK_Id_ActaRevision],
[Fecha],
[FechaCierre],
[Fk_Id_Empresa],
[Nit_Empresa],
[FK_Acta],
[Responsable],
Anio, 
Mes, 
AnioCierre, 
MesCierre,
ConsecutivoPlan,
[Elaborada],
[Observaciones]</CommandText>
      </Query>
