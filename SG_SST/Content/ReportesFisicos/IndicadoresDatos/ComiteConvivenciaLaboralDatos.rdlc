      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
Pk_Id_AccionActaConvivencia,
anio,
FechaProbable,
FechaCierre,
Responsable,
Nit_Empresa,
Mes, 
AnioCierre, 
MesCierre,
[AccionARealizar],
[Consecutivo_Acta],
[TemaReunion],
[Conclusiones],
[NombreEmpresa],
[NombreUsuario],
[Observaciones],
 CASE 
WHEN (FechaCierre) IS NULL THEN 'ABIERTAS' ELSE 'CERRADAS' END AS Estado_Plan, 
SUM(CASE WHEN FechaCierre  is null THEN 1 ELSE 0 END) AS SinCerrar,
SUM(CASE WHEN FechaCierre  is not null THEN 1 ELSE 0 END) AS SeCerro,

case when (FechaCierre is null) and (FechaProbable&gt;=GETDATE()) then 'Abierto-Vigente' 
 else 
 case when (FechaCierre is null) and (FechaProbable&lt;=GETDATE()) then 'Abierto-Vencido' 
   else
   case when (FechaCierre is not null) and (FechaProbable&gt;=FechaCierre) then 'Eficaz'   	
   else 'No Eficaz' 
end
end 
end as EstadoPlan,
count(*) as total
 from V_IndAccionesActaCovivencia_OrigenDatos	

group by
Pk_Id_AccionActaConvivencia,
anio,
FechaProbable,
FechaCierre,
Responsable,
Nit_Empresa,
Mes, 
AnioCierre, 
MesCierre,
[AccionARealizar],
[Consecutivo_Acta],
[TemaReunion],
[Conclusiones],
[NombreEmpresa],
[NombreUsuario],
[Observaciones]</CommandText>
