        <CommandText>SELECT 
Pk_Id_AccionActaCopasst,
FechaProbable,
FechaCierre,
Responsable,
Nit_Empresa,
Consecutivo_Acta,
[TemaReunion],
[Conclusiones],
[NombreUsuario],
[Observaciones],
 Anio, 
 Mes, 
 AnioCierre, 
 MesCierre,
 CASE 
WHEN (V_IndAccionesActaCopast_OrigenDatos.FechaCierre) IS NULL THEN 'ABIERTAS' ELSE 'CERRADAS' END AS Estado_Plan, 
SUM(CASE WHEN FechaCierre  is null THEN 1 ELSE 0 END) AS SinCerrar,
SUM(CASE WHEN FechaCierre  is not null THEN 1 ELSE 0 END) AS SeCerro,
 
case when (FechaCierre is null) and (FechaProbable&gt;=GETDATE()) then 'Abierto-Vigente' 
 else 
 case when (FechaCierre is null) and (FechaProbable&lt;=GETDATE()) then 'Abierto-Vencido' 
   else
   case when (FechaCierre is not null) and (FechaProbable&gt;=FechaCierre) then 'Eficaz'   
   else 'No Eficaz' 
end
end 
end as EstadoPlan,
count(*) as total
from   V_IndAccionesActaCopast_OrigenDatos					  
									  
group by
Pk_Id_AccionActaCopasst,
anio,
FechaProbable,
FechaCierre,
Responsable,
Nit_Empresa,
Consecutivo_Acta,
TemaReunion,
Conclusiones,
NombreUsuario,
Observaciones,
Anio, 
Mes, 
AnioCierre, 
MesCierre</CommandText>
