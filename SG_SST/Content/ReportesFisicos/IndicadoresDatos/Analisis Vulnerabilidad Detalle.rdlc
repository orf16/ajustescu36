    <DataSet Name="identificaAmenaza">
      <Query>
        <DataSourceName>SGSSTCERT</DataSourceName>
        <CommandText>SELECT DISTINCT 
                         e.Nit_Empresa, s.Nombre_Sede, 
                         CASE WHEN vidpera.tipo = 'N' THEN 'NATURALES' WHEN vidpera.tipo = 'S' THEN 'SOCIALES' WHEN vidpera.tipo = 'T' THEN 'TECNOLÓGICO' END AS tipo, 
                         vidpera.amenaza AS amena, 
                         CASE WHEN a.calificacion = 'I' THEN 'Inminente' WHEN a.calificacion = 'PR' THEN 'Probable' WHEN a.calificacion = 'P' THEN 'Posible' END AS calia, 
                         a.color AS coloa
FROM            Tbl_Eme_InfoGeneral AS ig INNER JOIN
                         Tbl_Sede AS s ON ig.fk_id_sede = s.Pk_Id_Sede INNER JOIN
                         Tbl_Empresa AS e ON s.Fk_Id_Empresa = e.Pk_Id_Empresa INNER JOIN
                         Tbl_vul_eme_IdentificacionAmenazas AS a ON e.Nit_Empresa = a.NitEmpresa INNER JOIN
                         Tbl_vul_Identificacion_Personas AS vidpera ON a.fk_id_amenaza = vidpera.pk_id_identificacion_amenazas
</CommandText>
      </Query>
