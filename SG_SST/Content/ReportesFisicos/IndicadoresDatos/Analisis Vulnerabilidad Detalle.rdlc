    <DataSet Name="personas">
      <Query>
        <DataSourceName>SGSSTCERT</DataSourceName>
        <CommandText>SELECT DISTINCT e.Nit_Empresa, s.Nombre_Sede, 

case when ape.tipo='O' then 'ORGANIZACIÓN'
when ape.tipo='C' then 'CAPACITACIÓN'

when ape.tipo='D' then 'DOTACIÓN'
end AS tipape, ape.aspectos AS aspe, pe.calificacion AS cape
FROM            dbo.Tbl_vul_Personas AS ape RIGHT OUTER JOIN
                         dbo.Tbl_Eme_InfoGeneral AS ig INNER JOIN
                         dbo.Tbl_Sede AS s ON ig.fk_id_sede = s.Pk_Id_Sede INNER JOIN
                         dbo.Tbl_Empresa AS e ON s.Fk_Id_Empresa = e.Pk_Id_Empresa INNER JOIN
                         dbo.Tbl_vul_eme_Personas AS pe ON e.Nit_Empresa = pe.NitEmpresa ON ape.pk_id_personas = pe.fk_id_aspecto
						 
</CommandText>
