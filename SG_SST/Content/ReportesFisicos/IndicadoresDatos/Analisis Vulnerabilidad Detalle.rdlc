    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SGSSTCERT</DataSourceName>
        <CommandText>SELECT DISTINCT 
                         e.Nit_Empresa, s.Nombre_Sede, amepe.tipo AS tiamen, amepe.amenaza AS amena, a.calificacion AS caliaman, a.color AS coloramen, ape.tipo AS tipape, 
                         ape.aspectos AS aspe, pe.calificacion AS cape, vre.tipo AS tiprec, vre.aspectos AS asrec, emere.calificacion AS calrec, vpro.tipo AS tippro, vpro.aspectos AS asppro, 
                         emepro.calificacion AS calpro
FROM            dbo.Tbl_vul_eme_sistemas_procesos AS emepro INNER JOIN
                         dbo.Tbl_vul_SistemasProcesos AS vpro ON emepro.fk_id_aspecto = vpro.pk_id_sistemas_procesos LEFT OUTER JOIN
                         dbo.Tbl_vul_Recursos AS vre INNER JOIN
                         dbo.Tbl_vul_eme_Recursos AS emere INNER JOIN
                         dbo.Tbl_Eme_InfoGeneral AS ig INNER JOIN
                         dbo.Tbl_Sede AS s ON ig.fk_id_sede = s.Pk_Id_Sede INNER JOIN
                         dbo.Tbl_Empresa AS e ON s.Fk_Id_Empresa = e.Pk_Id_Empresa INNER JOIN
                         dbo.Tbl_vul_eme_Personas AS pe ON e.Nit_Empresa = pe.NitEmpresa ON emere.NitEmpresa = e.Nit_Empresa INNER JOIN
                         dbo.Tbl_vul_Identificacion_Personas AS amepe INNER JOIN
                         dbo.Tbl_vul_eme_IdentificacionAmenazas AS a ON amepe.pk_id_identificacion_amenazas = a.fk_id_amenaza ON e.Nit_Empresa = a.NitEmpresa ON 
                         vre.pk_id_recursos = emere.fk_id_aspecto INNER JOIN
                         dbo.Tbl_vul_Personas AS ape ON pe.fk_id_aspecto = ape.pk_id_personas ON emepro.NitEmpresa = ig.NitEmpresa </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
