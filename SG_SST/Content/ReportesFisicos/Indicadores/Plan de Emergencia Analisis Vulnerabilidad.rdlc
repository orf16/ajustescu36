    <DataSet Name="em_vul_ame_tipo">
      <Query>
        <DataSourceName>SGSSTCERT</DataSourceName>
        <CommandText>SELECT DISTINCT e.Nit_Empresa, s.Nombre_Sede, COUNT(a.fk_id_amenaza) AS totaltipo, CASE WHEN p.tipo = 'T' THEN 'Tecnológicos' WHEN p.tipo = 'S' THEN 'Sociales' WHEN p.tipo = 'N' THEN 'Naturales' END AS tipo, a.color
FROM            dbo.Tbl_Eme_InfoGeneral AS ig INNER JOIN
                         dbo.Tbl_Sede AS s ON ig.fk_id_sede = s.Pk_Id_Sede INNER JOIN
                         dbo.Tbl_Empresa AS e ON s.Fk_Id_Empresa = e.Pk_Id_Empresa INNER JOIN
                         dbo.Tbl_vul_eme_IdentificacionAmenazas AS a ON a.NitEmpresa = e.Nit_Empresa INNER JOIN
                         dbo.Tbl_vul_eme_Consolidado ON a.fk_pk_id_plan_emergencia = dbo.Tbl_vul_eme_Consolidado.fk_pk_id_plan_emergencia INNER JOIN
                         dbo.Tbl_vul_Identificacion_Personas AS p ON a.fk_id_amenaza = p.pk_id_identificacion_amenazas where e.Nit_Empresa=@nitempresa  and s.Nombre_Sede=@Sede
GROUP BY e.Nit_Empresa, s.Nombre_Sede, p.tipo, a.color </CommandText>
      </Query>
