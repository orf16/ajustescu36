      <Query>
        <DataSourceName>SGSSTCERT</DataSourceName>
        <CommandText>SELECT    DISTINCT    e.Nit_Empresa, s.Nombre_Sede, dbo.Tbl_vul_eme_Consolidado.organizacion, dbo.Tbl_vul_eme_Consolidado.capacitacion, dbo.Tbl_vul_eme_Consolidado.dotacion,
                          dbo.Tbl_vul_eme_Consolidado.materiales, dbo.Tbl_vul_eme_Consolidado.edificacion, dbo.Tbl_vul_eme_Consolidado.equipos, 
                         dbo.Tbl_vul_eme_Consolidado.servicios_publicos, dbo.Tbl_vul_eme_Consolidado.sistemas_alternos, dbo.Tbl_vul_eme_Consolidado.recuperacion, 
                         dbo.Tbl_vul_eme_Consolidado.calificacion_personas, dbo.Tbl_vul_eme_Consolidado.interpretacion_personas, dbo.Tbl_vul_eme_Consolidado.color_personas, 
                         dbo.Tbl_vul_eme_Consolidado.calificacion_recursos, dbo.Tbl_vul_eme_Consolidado.interpretacion_recursos, dbo.Tbl_vul_eme_Consolidado.color_recursos, 
                         dbo.Tbl_vul_eme_Consolidado.calificacion_sistemas_procesos, dbo.Tbl_vul_eme_Consolidado.interpretacion_sistemas_procesos, 
                         dbo.Tbl_vul_eme_Consolidado.color_sistemas_procesos
FROM            dbo.Tbl_Eme_InfoGeneral AS ig INNER JOIN
                         dbo.Tbl_Sede AS s ON ig.fk_id_sede = s.Pk_Id_Sede INNER JOIN
                         dbo.Tbl_Empresa AS e ON s.Fk_Id_Empresa = e.Pk_Id_Empresa INNER JOIN
                         dbo.Tbl_vul_eme_IdentificacionAmenazas AS a ON e.Nit_Empresa = a.NitEmpresa INNER JOIN
                         dbo.Tbl_vul_eme_Consolidado ON a.fk_pk_id_plan_emergencia = dbo.Tbl_vul_eme_Consolidado.fk_pk_id_plan_emergencia
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
