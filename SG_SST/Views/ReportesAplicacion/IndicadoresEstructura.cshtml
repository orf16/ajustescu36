                                                  --Seleccionar--
                                              </option>

                                              <option value="1">Enfermedad General</option>
                                              <option value="2">Enfermedad Laboral</option>
                                              <option value="3">Accidente de Trabajo</option>

                                          </select>
                                      </div>
                                  </div>
                              </div>
                          </div>
