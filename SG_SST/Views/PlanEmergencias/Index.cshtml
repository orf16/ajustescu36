@model SG_SST.Models.Emergencias.EmergenciasModel

@{
    ViewBag.Title = "Plan de Emergencia";
    Layout = "~/Views/Shared/_Layout.cshtml";

}


<link href="~/Content/Site.css" rel="stylesheet" />
<!-- Bootstrap core CSS -->
<link href="~/Content/miga-de-pan.css" rel="stylesheet">
<!-- Referencia a botones -->
<link rel="stylesheet" href="~/Content/css/specialbuttons.css">

<style>
    .ui-widget-content a {
		color: !important;
	}
    .noborder {
        border: none;
        padding: 4px 4px 4px 4px;
    }

    .custom_text {
        height: 27px;
    }

    .rombo {
        width: 15px;
        height: 15px;
        border: 1px solid #555;
        margin: 5px 5px 5px 32px;
        -webkit-transform: rotate(45deg);
        -moz-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
        -o-transform: rotate(45deg);
        transform: rotate(45deg);
    }

    input[type=file] {
        color: transparent;
        /*width: 77%;*/
        height: 8%;
    }
    
    .rdiv {
        transform: rotate(-45deg);
        -ms-transform: rotate(-45deg);
    }

    .animation-1 {
        width: 55px;
        margin: 5% auto;
        transform: rotate(45deg);
        -ms-transform: rotate(45deg);
    }

    .animation-1 div {
        display: inline-block;
        border: 1px solid #000000;
        /*background-color: #0088d0;*/
        width: 25px;
        height: 25px;
        margin: 0;
        padding: 0;
    }
    .nombreArchivo{
       font-weight: bold;
        font-size:90%;
        color:blue;

    }


    .animation-0 div {
        display: inline-block;
        border: none;
        width: auto;
        height: auto;
        margin: 0;
        padding: 0;
    }


    /*.animation-0 .box0 {
        border: none;
        -webkit-animation: animation1-box0 1.5s infinite;
        animation: animation1-box0 2s infinite;
    }*/
    
    .animation-1 .box1 {
        -webkit-animation: animation1-box1 1.5s infinite;
        animation: animation1-box1 2s infinite;
    }

    .animation-1 .box2 {
        animation: animation1-box2 1.5s infinite;
        animation: animation1-box2 2s infinite;
    }

    .animation-1 .box3 {
        animation: animation1-box3 1.5s infinite;
        animation: animation1-box3 2s infinite;
    }

    .animation-1 .box4 {
        animation: animation1-box4 1.5s infinite;
        animation: animation1-box4 2s infinite;
    }

 #mdialTamanio {
      width: 80% !important;
      height:80%;
    }

</style>

<ol class="breadcrumb">
    <li><a href="@Url.Action("Index", "Home")">Planificación</a></li>
    <li><a href="javascript:void(0)">Prevención, preparación y respuesta ante emergencias</a></li>
    <li class="active">Generar Plan de Emergencia</li>
</ol>


<div class="panel panel-default std-panel-body" style="background-color: rgba(196, 202,196, 0.1)">
    <div class="panel-heading" style="color:white">
        GENERAR PLAN DE EMERGENCIA
    </div>

    <div class="panel-body form-horizontal">
