              }
              else
              {                   
                    <td class="noborder">
                        @Html.DropDownList("IdSede", null, htmlAttributes: new { @class = "form-control" }) </td>                  
              }
                   
                    <td class="noborder">
                        <button type="button" id="generarPlan"class="boton botonactive" onclick="GenerarPlanEmergenciaSede()">Generar</button>
                    </td>
                </tr>
            </table>

